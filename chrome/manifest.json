{
    "manifest_version": 2,

    "name": "JFP",
    "description": "Web print at Carleton College made easy.",
    "version": "1.0",

    "permissions": [
        "https://print.ads.carleton.edu:9192/",
        "tabs",
        "geolocation"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "JFP"
    }
}
